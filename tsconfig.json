{
  "compilerOptions": {
    "baseUrl": ".",
    "lib":["ESNext","DOM"],
    "module": "ESNext",
    "moduleResolution":"node",
    "paths": {
      "@/*":["src/*"]
    },
    "esModuleInterop": true,
    "skipLibCheck": true,
    "strict": true,
    "types": ["vite/client", "node", "vitest/globals"]
  },
  "include": [ "src/**/*.ts",
  "src/**/*.d.ts",
  "src/**/*.tsx",
  "src/**/*.vue"]
}